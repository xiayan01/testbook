{
    "title": "编写docker学习笔记",
    "description": "编写docker学习笔记",
    "author": "Dazzle",
    "language": "zh-hans",
    "root": ".",
	"plugins":[
		"-search",
		"search-pro",
		"github",
        "chapter-fold",
        "code",
		"copy-code-button",
        "splitter",
		"page-footer-ex",
		"simple-page-toc",
		"anchor-navigation-ex"
    ],
    "pluginsConfig":{
		"search-pro": {
			"cutWordLib": "nodejieba",
			"defineWord" : ["Gitbook Use"]
		},
        "page-footer-ex": {
            "copyright": "By <em>Dazzle Lin</em>",
            "markdown": false,
            "update_label": "<i>updated</i>",
            "update_format": "YYYY-MM-DD HH:mm:ss"
        },
		"simple-page-toc": {
            "maxDepth": 3,
            "skipFirstH1": true
        },
		"github": {
			"url": "https://github.com/linqinye"
		},
		"anchor-navigation-ex": {
			"showLevel": false,
			"multipleH1":false,
			"float": { 
				"showLevelIcon": true,
				"level1Icon": "fa fa-hand-o-right",
				"level2Icon": "fa fa-hand-o-right",
				"level3Icon": "fa fa-hand-o-right"
			}
		}
    }
}
